
<div class="row">
  <div class="column">
    <div id="creation">

      <center><%=button_to"Create New User","/register", method: :get %></center>
      <center><%=button_to"Log In", login_path, method: :get %></center>

    </div>
  </div>
</div>
<div id= "all_users">
  <strong><h3>Existing Users</h3></strong>
  <%@users.each do |user|%>
  <div id= <%="existing_users-#{user.id}"%>>
    <%=link_to user.email, user_path(user), method: :get%>
  <%end%>
  </div>
</div>